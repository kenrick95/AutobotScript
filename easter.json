{
    "manifest_version": 1,
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "HelloWorld.user.js" ],
        "matches":          [   "http://stackoverflow.com/*",
                                "https://stackoverflow.com/*"
                            ],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true,
    "description":  "My first sensibly named script!",
    "name":         "Hello World",
    "version":      "1"
}